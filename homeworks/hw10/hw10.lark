rstring: "r\"" regex* "\""

group: "(" regex ")"
pipe: regex "|" regex
?regex: character | word | group | pipe | class

range: NUMBER "-" NUMBER | LETTER "-" LETTER
class: "[" (character* | range*)+ "]"

character: LETTER | NUMBER
word: WORD

%ignore /\s+/
%import common.LETTER
%import common.NUMBER
%import common.WORD

